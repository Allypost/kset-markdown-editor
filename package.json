{
  "name": "markdown-editor",
  "version": "0.0.1",
  "description": "Simple markdown editor",
  "private": true,
  "scripts": {
    "start": "./node_modules/.bin/nodemon --watch src/**/* server.js --exec \"cls && babel-node\"",
    "build": "./node_modules/.bin/webpack --config webpack.config.js",
    "watch-spooky": "./node_modules/.bin/webpack-dev-server --mode development",
    "watch": "./node_modules/.bin/webpack --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ."
  },
  "main": "server.js",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.15.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.0",
    "csurf": "^1.9.0",
    "debug": "~4.1.0",
    "ejs": "^2.6.1",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "http-errors": "~1.7.1",
    "lodash": "^4.15.0",
    "morgan": "~1.9.0",
    "normalize.css": "^8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "autoprefixer": "^9.3.1",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-transform-async-to-module-method": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.7",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.4",
    "nodemon": "^1.18.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pixrem": "^4.0.1",
    "postcss-font-magician": "^2.2.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.2.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-closure-compiler": "^2.1.6"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "no-multi-spaces": 0,
      "no-console": 0,
      "indent": 0,
      "curly": 0,
      "new-cap": 0,
      "no-shadow": 0,
      "import/prefer-default-export": 0,
      "no-param-reassign": 0
    },
    "env": {
      "mocha": true
    }
  },
  "author": "Josip Igrec <me@allypost.net>",
  "license": "ISC"
}
